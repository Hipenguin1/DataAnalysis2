    "sql ='''\n",
    "SELECT r.Continent, COUNT(*), rount(AVG(l.population)) FROM city AS l\n",
    "\tJOIN country AS r ON l.Countrycode = r.code \n",
    "\tGROUP BY r.Continent \n",
    "\tORDER BY AVG(l.population) desc; \n",
    "'''"
